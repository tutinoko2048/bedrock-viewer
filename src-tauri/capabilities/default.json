{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "dialog:allow-message",
    "dialog:allow-open",
    "fs:default",
    "fs:allow-read",
    "fs:allow-read-dir",
    "fs:allow-read-file",
    "fs:read-dirs",
    "fs:allow-read-text-file"
  ]
}
